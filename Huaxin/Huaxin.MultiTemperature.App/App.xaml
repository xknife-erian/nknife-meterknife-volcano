<Application x:Class="Huaxin.MultiTemperature.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ioC="clr-namespace:Huaxin.MultiTemperature.ViewModels.IoC;assembly=Huaxin.MultiTemperature.ViewModels"
             StartupUri="/Views/Workbench.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Buttons.xaml" />
                <ResourceDictionary Source="/Resources/TabControls.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ioC:ViewModelLocator x:Key="Locator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
