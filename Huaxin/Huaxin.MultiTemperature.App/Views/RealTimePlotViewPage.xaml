<UserControl x:Class="Huaxin.MultiTemperature.App.Views.RealTimePlotViewPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
            Height="500" Width="800">
    <UserControl.DataContext>
        <Binding Path="RealTimePlotView" Source="{StaticResource Locator}" />
    </UserControl.DataContext>
    <Grid Name="MainGrid" Margin="0">
        <oxy:PlotView Name="MainPlotView" Margin="0,0,20,0"
                      />
    </Grid>
</UserControl>