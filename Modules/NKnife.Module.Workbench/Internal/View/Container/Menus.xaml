<UserControl x:Class="NKnife.Module.Workbench.Internal.View.Container.Menus"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:rf="clr-namespace:Fluent;assembly=RAY.Plugins.WPF"
             xmlns:fluent="urn:fluent-ribbon"
             xmlns:container="clr-namespace:NKnife.Module.Workbench.Internal.ViewModels.Container"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance {x:Type container:MenusVm}}"
             d:DesignHeight="150" d:DesignWidth="600">
    <fluent:Ribbon x:Name="_RibbonMenu_">
        <b:Interaction.Behaviors>
            <rf:RibbonTabBehaviour TabItemsSource="{Binding CategoryMenus}" />
        </b:Interaction.Behaviors>
    </fluent:Ribbon>
</UserControl>